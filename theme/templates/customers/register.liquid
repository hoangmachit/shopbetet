<div class="page__register">
  <nav class="main__breadcumb" aria-label="Breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item">
        <a href="/">Trang chủ</a>
      </li>
      <li class="breadcrumb-item">
        <a href="/account">Tài khoản</a>
      </li>
      <li class="breadcrumb-item active" aria-current="page">Đăng ký</li>
    </ol>
  </nav>
  <div class="page__register--form">
    <div class="wrapper__form">
      {% form 'create_customer' %}
        <div class="page__register--heading">
          <h1>Đăng ký tài khoản</h1>
        </div>
        {{ form.errors | default_errors }}
        <div class="c-form">
          <label for="customer_first_name" class="c-label">First name</label>
          <input
            type="text"
            class="c-control"
            placeholder=""
            name="customer[first_name]"
            id="customer_first_name"
            autocomplete="off"
            size="30" />
        </div>
        <div class="c-form">
          <label for="customer_last_name" class="c-label">Last name</label>
          <input
            type="text"
            class="c-control"
            placeholder=""
            name="customer[last_name]"
            autocomplete="off"
            id="customer_last_name"
            size="30" />
        </div>
        <div class="c-form">
          <label for="customer_email" class="c-label">Email</label>
          <input
            type="text"
            class="c-control"
            placeholder="Email"
            name="customer[email]"
            autocomplete="off"
            id="customer_email" />
        </div>
        <div class="c-form">
          <label for="customer_password" class="c-label">Mật khẩu</label>
          <input
            type="password"
            class="c-control"
            placeholder="******"
            name="customer[password]"
            autocomplete="off"
            id="customer_password"
            size="30" />
        </div>
        <div class="c-form">
          <button class="btn btnRegister" type="submit">
            Đăng ký tài khoản
          </button>
        </div>
        <div class="c-register">
          <p>Đã có tài khoản đăng nhập</p>
          <a href="/account/login">tại đây.</a>
        </div>
      {% endform %}
      <div class="formRegisterSocial">
        <div class="formRegisterSocial__heading">
          <h3 class="formRegisterSocial__heading--title">Đăng ký bằng tài khoản khác</h3>
        </div>
        <div class="formRegisterSocial__list">
          <ul>
            <li>
              <form
                action="#"
                role="form"
                method="POST">
                <button
                  type="submit"
                  class="btn__social"
                  disabled><img src="data:image/svg+xml;base64," alt="">Đăng ký Google</button>
              </form>
            </li>
            <li>
              <form
                action="#"
                role="form"
                method="POST">
                <button
                  type="submit"
                  class="btn__social"
                  disabled><img src="data:image/svg+xml;base64," alt="">Đăng nhập Facebook</button>
              </form>
            </li>
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>